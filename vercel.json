{
  "buildCommand": "cd spicy-fairytales && npm ci && npm run build -- --configuration production-spa",
  "outputDirectory": "spicy-fairytales/dist/spicy-fairytales/browser",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}