{
  "version": 2,
  "name": "spicy-fairytales",
  "buildCommand": "cd spicy-fairytales && npm run build",
  "outputDirectory": "spicy-fairytales/dist/spicy-fairytales/browser",
  "installCommand": "cd spicy-fairytales && npm ci",
  "framework": "angular",
  "functions": {
    "spicy-fairytales/dist/spicy-fairytales/server/server.mjs": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/[...api].js"
    },
    {
      "source": "/(.*)",
      "destination": "/spicy-fairytales/dist/spicy-fairytales/server/server.mjs"
    }
  ],
  "env": {
    "VITE_USE_MOCKS": "false"
  },
  "build": {
    "env": {
      "VITE_USE_MOCKS": "false"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}